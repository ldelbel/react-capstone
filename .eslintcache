urn filter;\n    }\n  };\n\n  const handleSubmit = () => {\n    props.handleUpdateFilter(filter);\n  };\n\n  return (\n    <div style={{\n      height: 120, background: '#343a40', borderTopLeftRadius: 20, borderTopRightRadius: 20, padding: 15, paddingRight: 30, paddingLeft: 30,\n    }}\n    >\n      <div><span style={{ fontFamily: '\"Alfa Slab One\", cursive', color: 'var(--white)', fontSize: 30 }}>Filters&nbsp;</span></div>\n      <div className=\"d-flex flex-row justify-content-center align-items-center\" style={{ height: 50 }}>\n        <div style={{ marginRight: 20 }}>\n          <span style={{\n            fontFamily: 'Alata, sans-serif', color: 'var(--light)', fontSize: 20, marginRight: 10,\n          }}\n          >\n            Price Range&nbsp;\n          </span>\n          <select\n            style={{ borderRadius: 8 }}\n            defaultValue=\"null\"\n            onChange={e => handleChange(e.target)}\n            name=\"price\"\n          >\n            <option key=\"uniq\" value=\"null\">\n              Select\n            </option>\n            {PRICES.map(price => (\n              <option\n                key={price}\n                value={price}\n              >\n                {`< ${price}`}\n              </option>\n            ))}\n\n          </select>\n        </div>\n        <div style={{ marginRight: 20 }}>\n          <span style={{\n            fontFamily: 'Alata, sans-serif', color: 'var(--light)', fontSize: 20, marginRight: 10,\n          }}\n          >\n            Sector&nbsp;\n          </span>\n          <select\n            style={{ borderRadius: 8 }}\n            onChange={e => handleChange(e.target)}\n            name=\"sector\"\n            defaultValue=\"null\"\n          >\n            <option key=\"uniq\" value=\"null\">\n              Select\n            </option>\n            {SECTORS.map(sector => (\n              <option key={sector} value={sector}>{sector}</option>\n            ))}\n          </select>\n        </div>\n        <div style={{ marginRight: 20 }}>\n          <span style={{\n            fontFamily: 'Alata, sans-serif', color: 'var(--light)', fontSize: 20, marginRight: 10,\n          }}\n          >\n            Country&nbsp;\n          </span>\n          <select\n            style={{ borderRadius: 8 }}\n            onChange={e => handleChange(e.target)}\n            name=\"country\"\n            defaultValue=\"null\"\n          >\n            <option key=\"uniq\" value=\"null\">\n              Select\n            </option>\n            {COUNTRIES.map(country => (\n              <option key={country} value={country}>{country}</option>\n            ))}\n          </select>\n        </div>\n        <div style={{ marginRight: 20 }}>\n          <span style={{\n            fontFamily: 'Alata, sans-serif', color: 'var(--light)', fontSize: 20, marginRight: 10,\n          }}\n          >\n            Exchange&nbsp;\n          </span>\n          <select\n            style={{ borderRadius: 8 }}\n            onChange={e => handleChange(e.target)}\n            name=\"exchange\"\n            defaultValue=\"null\"\n          >\n            <option key=\"uniq\" value=\"null\">\n              Select\n            </option>\n            {EXCHANGES.map(exchange => (\n              <option key={exchange} value={exchange}>{exchange}</option>\n            ))}\n          </select>\n        </div>\n        <button\n          className=\"btn\"\n          type=\"button\"\n          onClick={handleSubmit}\n          style={{\n            fontFamily: 'Alata, sans-serif', fontSize: 20, color: 'var(--white)', background: 'var(--yellow)', borderStyle: 'none', paddingTop: 3, paddingBottom: 6, fontWeight: 'bold', borderRadius: 15,\n          }}\n        >\n          Apply\n        </button>\n      </div>\n    </div>\n  );\n};\n\nFilter.propTypes = {\n  handleUpdateFilter: PropTypes.func.isRequired,\n};\n\nexport default Filter;\n","/home/lucas/programming/microverse/react/react-capstone/src/actions/i